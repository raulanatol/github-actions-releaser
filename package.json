{
  "name": "github-actions-releaser",
  "version": "2.1.0",
  "description": "This action generate a release notes from closed issues",
  "keywords": [
    "actions",
    "node",
    "publish",
    "release"
  ],
  "homepage": "https://github.com/raulanatol/github-actions-releaser#readme",
  "bugs": {
    "url": "https://github.com/raulanatol/github-actions-releaser/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raulanatol/github-actions-releaser.git"
  },
  "license": "MIT",
  "author": "raulanatol",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "clean": "shx rm -rf dist lib",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@actions/core": "1.9.1",
    "@actions/github": "5.0.0"
  },
  "devDependencies": {
    "@cowcoders/eslint-config": "1.2.11",
    "@octokit/rest": "18.12.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.4",
    "@typescript-eslint/eslint-plugin": "5.1.0",
    "@typescript-eslint/parser": "5.1.0",
    "@vercel/ncc": "0.31.1",
    "eslint": "8.1.0",
    "eslint-plugin-jest": "25.2.2",
    "jest": "27.3.1",
    "jest-circus": "27.3.1",
    "js-yaml": "4.1.0",
    "shx": "0.3.3",
    "ts-jest": "27.0.7",
    "typescript": "4.4.4"
  }
}
