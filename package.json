{
  "name": "github-actions-releaser",
  "version": "2.3.0",
  "description": "This action generate a release notes from closed issues",
  "keywords": [
    "actions",
    "node",
    "publish",
    "release"
  ],
  "homepage": "https://github.com/raulanatol/github-actions-releaser#readme",
  "bugs": {
    "url": "https://github.com/raulanatol/github-actions-releaser/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/raulanatol/github-actions-releaser.git"
  },
  "license": "MIT",
  "author": "raulanatol",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "clean": "shx rm -rf dist lib",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1"
  },
  "devDependencies": {
    "@cowcoders/eslint-config": "2.0.0",
    "@octokit/rest": "19.0.7",
    "@types/jest": "29.4.0",
    "@types/node": "18.13.0",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "@vercel/ncc": "0.36.1",
    "eslint": "8.34.0",
    "eslint-plugin-jest": "27.2.1",
    "jest": "29.4.2",
    "jest-circus": "29.4.2",
    "js-yaml": "4.1.0",
    "shx": "0.3.4",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "engines": {
    "node": "=18.14.0",
    "npm": "=9.3.1"
  },
  "engineStrict": true
}
